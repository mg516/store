/* @import '../../../../utils/appList.wxss'; */
@import '../../../../utils/appList_details.wxss';
.contTl>view {
  font-size: 26rpx;
}
.mainBox {
  margin-top: 0!important;
  padding-top: 180rpx;
  width: 750rpx;
  /* height: 100vh; */
  box-sizing: border-box;
}
.mainBox>scroll-view{
  width: 100%;
  height: 100%;
}

.searchText{
  text-align: center;
  font-size: 34rpx;
}

.serach-box{
  width:100%;
  /* box-shadow: 0 0 2px #e5e5e5; */

  background-color: #fff;
  /* padding: 10rpx 0; */
  position: fixed;
  top: 0;
  left: 0; 
  z-index: 1;
}
.list-search{
  width: 690rpx;
  margin: 0 auto 0;
  position: relative;  
  border-bottom:2rpx solid #bfbfbf;
}
.search-icon{
  width:30rpx;
  height:30rpx;
  position: absolute;
  top:30rpx;
  left:10rpx;
  margin:5rpx;
}
.search-input{
  font-size:30rpx;
  height:90rpx;
  line-height: 90rpx;
  padding-left:60rpx;
  width:80%;
}
.delect-icon{
  width:30rpx;
  height:30rpx;
  position: absolute;
  top:30rpx;
  right:10rpx;
  margin:5rpx;
}

/* 选择时间 */
.list-time{
  width: 490rpx;
  height:60rpx;
  line-height:60rpx;
  margin: 20rpx auto 14rpx;
  border-radius:20rpx;
  padding:5rpx 0;
}
.time-title{
  font-size:32rpx;
  float:left;
}
.time-title image{
  width:40rpx;
  height:40rpx;
  display:blcok;
  margin:10rpx 10rpx 10rpx 20rpx;
}
.time-picker{
  float:left;
  color:#222;
  font-size:30rpx;
  margin:0 10rpx;
  width:180rpx;
  text-align:center;
  font-weight:bold;
}
/* 选择商品 */
.picker{
  max-width: 600rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  line-height: 66rpx;
}
.picker,.picIcon{
  display: inline-block;
  vertical-align: middle;
}
.time-text{
  float:left;
  color:#808080;
  font-size:30rpx;
} 
.picIcon{
  width: 22rpx;
  height: 22rpx;
  padding: 0 10rpx
}

.contTl>view:nth-child(1) {
  line-height:70rpx;
  height:70rpx;
}

.nomore{
  text-align:center;
  font-size:28rpx;
  color:#666;
  line-height:40rpx;
  padding-bottom:20rpx;
}