.group_top{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1;
  width: 750rpx;
  /* height: 200rpx; */
  background-color: #fff;
}

.list-search{
  width: 700rpx;
  margin: 0 auto 0;
  position: relative;  
  border-bottom:2rpx solid #F7F7F7;
}
.search-icon{
  width:30rpx;
  height:30rpx;
  position: absolute;
  top:30rpx;
  left:10rpx;
  margin:5rpx;
}
.search-input{
  font-size:28rpx;
  height:90rpx;
  line-height: 90rpx;
  padding-left:60rpx;
  width:80%;
}
.delect-icon{
  width:30rpx;
  height:30rpx;
  position: absolute;
  top:30rpx;
  right:10rpx;
  margin:5rpx;
}

.list-title{width: 700rpx;border-bottom:2rpx solid #F7F7F7;margin: 0 auto;}
.list-title>view{display: inline-block;width: 350rpx;text-align: center;font-size: 30rpx;color: #2F2F2F;line-height: 90rpx;}
.list-title>.callphone{width: 160rpx;}
.list-title>.addOrder{width: 160rpx;}

.group_cont{margin-top: 200rpx;}
.userLi{ width: 700rpx;margin: 0 auto;}
.userLi>view{ display: inline-block;width: 350rpx;text-align: center;font-size: 30rpx;vertical-align: middle;line-height: 80rpx;}
.userIndex{ display: inline-block;font-size: 26rpx;color: #ccc;width: 76rpx;}
.userName{ display: inline-block;text-align: center;padding-left: 10rpx;}
.userLi>.callphone{ width: 40rpx;height: 40rpx;padding: 0 60rpx; }
.userLi>.addOrder{ width: 40rpx;height: 40rpx;padding: 0 60rpx; }

.addUser{width: 80rpx;height: 80rpx;position: fixed;bottom: 80rpx;left: 40rpx;}
