/* pages/request-search/request-search.wxss */
page{
  padding-top: 100rpx;
}
.serach-box{
  width:100%;
  padding:10rpx 0;
  /* box-shadow: 0 0 10px #e5e5e5; */

  position:fixed;
  top:0;
  background-color:#fff;
}
.list-search{
  width: 94%;
  height:60rpx;
  margin: 15rpx auto;
  background-color:#f1f1f1;
  border-radius:20rpx;
  position: relative;  
  padding:5rpx 0;
}
.search-icon{
  width:40rpx;
  height:40rpx;
  position: absolute;
  top:10rpx;
  left:10rpx;
  margin:5rpx;
}
.search-input{
  font-size:30rpx;
  height:60rpx;
  line-height: 60rpx;
  padding-left:60rpx;
  width:80%;
}
.delect-icon{
  width:40rpx;
  height:40rpx;
  position: absolute;
  top:10rpx;
  right:10rpx;
  margin:5rpx;
}

.select-view{
  margin:20rpx 30rpx;
  padding:20rpx 40rpx;
  border-radius:20rpx;
  box-shadow: 0 0 10px #e5e5e5;
}

.list-box{
  padding:15rpx 0;
  font-size:32rpx;
}
.big-size{
  font-size:34rpx;
}
.list-box1{
  padding:15rpx 0;
  font-size:28rpx;
}
.list-left{
  float:left;
}
.list-right{
  float:right;
}
.box-style{
  color:#FC7903;
  font-size:32rpx;
}
.color-style{
  color:#808080;
}