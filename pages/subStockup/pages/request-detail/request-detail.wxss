@import '../../../../utils/appList_details.wxss';
.order-view>view{
  display: inline-block;
  vertical-align: middle;

  padding-left:6px;
  padding-right:6px;
  box-sizing:border-box;
}
.order-list{
  color: #666;
}
.order-list>text{
  color: #000;
}
.order-list:nth-child(2n-1){
  width: 310rpx;
  /* overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap; */
}
.order-list:nth-child(2n){
  width: 300rpx;
}
.handlePick{
  line-height: 60rpx;
  background-color: #24ae5f;
  color: #fff;
  font-size: 28rpx;
  border-radius: 30rpx;
  width: 160rpx;
  text-align: center;
  margin: 30rpx auto 0;
}
