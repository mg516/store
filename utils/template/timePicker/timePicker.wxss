.bottomLine{
  /* height:1px; */
  width:700rpx;
  margin:0 auto;
  background-color:#ccc;
}
/* 一级模块 */
.viewShow{
  width: 750rpx;
  height: 300rpx;
  background-color: #199CFB;
  display: flex;
  flex-wrap:nowrap;
}
.countShow{
  width: 750rpx;
  box-sizing:border-box;
}
.tableShow{
  width: 750rpx;
  /* min-height: 560rpx; */
  /* border: 1rpx solid #F6FB0A; */
  box-sizing: border-box;
  position: relative;
}
/* 次级模块 */
/* 一 */
.viewShow .viewLeft{
 width: 220rpx;
 text-align: center;
 padding-top:35px;
}
.viewShow .viewCenter{
 width: 310rpx;
}
.viewShow .viewRight{
 width: 220rpx;
 text-align: center;
 padding-top:35px;
}
/* 二 */
.countShow>.countI{
  display: flex;
  flex-wrap: nowrap;
  /* border-bottom: 1rpx solid #199CFB; */
  box-sizing:border-box;
  justify-content: center;
}
.countShow>view .countItem{
  width: 375rpx;
  height: 120rpx;
  box-sizing:border-box;
}
.countShow>view .countItem:nth-of-type(1){
  /* border-right: 1px solid #199CFB; */
}
.tableShow .loadingModal{
  position: absolute;
  background-color: rgba(0,0,0,0);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 20;
}
.loading{
  margin: 200rpx 300rpx;
  width:150rpx;
  height:150rpx;
}
.loadingModal>.loading{
  display:inline-block;
  vertical-align:middle;
  -webkit-animation:wx-button-loading-animate 1s steps(12, end) infinite;
  animation:wx-button-loading-animate 1s steps(12, end) infinite;
  background:transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size:100%;
}
/* 三 */
.tableBar{
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  text-align: center;
  padding: 15rpx 0;
}
.tableBar>view{
  width: 160rpx;
  border-radius: 5rpx;
  margin: 0 10rpx;
  background-color: #F3F3F3;
  font-size:25rpx;
  line-height:50rpx;
  color: #444;
}
.tableBar>view.activeBar{
  background-color: #4FB5F7;
  color: #fff;
}
/* 三级模块 */
.viewIcon{
  height: 70rpx;
}
.viewIcon image{
  width: 60rpx;
  height: 60rpx;
}
.viewText{
  font-size: 25rpx;
  color: #fff;
}
.viewVal{
  font-size: 30rpx;
  color: #F6FB0A;
}

.countItem{
  display: flex;
  flex-wrap: nowrap;
  padding: 20rpx;
}
.countLeft{
  width: 100rpx;
  text-align: right;
}
.countRight{
  width: 275rpx;
}
.countLeft>image{
  width: 70rpx;
  height: 70rpx;
}
.countTitle{
  color: #69798B;
  font-size: 28rpx;
  text-align: center;
  line-height: 35rpx;
}
.countVal{
  font-size: 32rpx;
  text-align: center;
  line-height: 55rpx;
}


/* 时间控件 */
/* pages/calendar/calendar.wxss */
.box {
    display: flex;
}

.box-lr {
    flex-direction: row;
}

.box-rl {
    flex-direction: row-reverse;
}

.box-tb {
    flex-direction: column;
}

.box-pack-center {
    justify-content: center;
}

.box-align-center {
    align-items: center;
}

.flex {
    flex-grow: 1;
}
.bg {
  background-image: linear-gradient(to bottom, #faefe7, #ffcbd7);
  overflow: hidden;
}
.pink-color {
  color: #ff629a;
}
.blue-color{
  color: #3257DB;
}
.fs28 {
  font-size: 28rpx;
}
.top-handle {
    height: 80rpx;
    width: 750rpx;
}
.prev {
    text-align: right;
    height: 80rpx;
}
.next {
    height: 80rpx;
}
.prev-handle {
    width: 80rpx;
    height: 100%;
}
.next-handle {
    width: 80rpx;
    height: 100%;
}
.date-area {
    width: 60%;
    height: 80rpx;
    text-align: center;
}

.chartBox{
  width: 100%;
  height: 400rpx;
  margin-top:30rpx;
}